    "class User:\n",
    "    def __init__(self):\n",
    "        self.liked = []\n",
    "        self.disliked = []\n",
    "        self.seen = []\n",
    "        self.all = []\n",
    "    \n",
    "    def recommend(self, similarities, n = 1):\n",
    "        similarities = similarities.drop(index = self.liked + self.seen, errors = 'ignore')\n",
    "        return similarities.nlargest(n, 0).index.values[-1]"
   ]
