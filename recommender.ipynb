    "        self.all += [movieId]\n",
    "        self.users[userId].all += [movieId]\n",
    "        \n",
    "    def addSeen(self, movieId, userId):\n",
    "        self.tags.loc[movieId, 'liked'] += 1/(self.n_users-1)\n",
    "        self.seen += [movieId]\n",
    "        self.users[userId].seen += [movieId]\n",
    "        self.all += [movieId]\n",
    "        self.users[userId].all += [movieId]\n",
    "        \n",
    "    def match(self):\n",
